{
  "name": "Bulk Image Downloader",
  "description": "Download every Image on a web page",
  "version": "0.10",
  "browser_action": {
    "default_popup": "popup/index.html"
  },
  "author": "Adam Mohammed",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["*://www.google.com/*"],
    "js": ["content.js"]
  }],
  "manifest_version": 2
}