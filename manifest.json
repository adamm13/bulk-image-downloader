{
  "name": "Simple Youtube Downloader",
  "description": "Downloaded Youtube Videos made easy",
  "version": "1.0",
  "author": "Adam Mohammed",
  "manifest_version": 2,
  "web_accessible_resources": ["src/youtubedl.js"],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/watch*"],
      "js": ["src/loader.js"]
    }
  ]
}